        <div class="flex items-center justify-between">
            <i class="fa-light fa-user-plus"></i>
            <div>
                <span class="text-sm">{{ member.firstName }} {{ member.lastName }}</span><br><span class="text-xs text-slate-500">{{ member.zipcode }} {{ member.city }}</span>
            </div>
        </div>
    </a>
{% else %}
    <div class="grid grid-cols-1 px-2 p-1">
        <p>{{ members|length }} utilisateur trouv√©.</p>
    </div>
{% endfor %}